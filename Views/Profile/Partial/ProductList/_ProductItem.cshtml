@using ketnoicungcau.business.Enums
@model ketnoicungcau.business.ModelWeb.ProductWeb

@{
    if (Model != null)
    {
        switch (Model.Type)
        {
            case ProductType.Buy:
                @await Html.PartialAsync("~/Views/Profile/Partial/ProductList/_RowBuy.cshtml", Model)
                break;
            case ProductType.Sell:
                @await Html.PartialAsync("~/Views/Profile/Partial/ProductList/_RowSell.cshtml", Model)
                break;
        }
    }
}